#Create a datebase
 mysql -h 172.17.0.2 -uroot -pmypassword
 show databases;
 create database books;
 show databases;
 use books;
 show tables;
 create table goodread(id int(1,1) unsigned auto_increment primary key not null,title varchar(255) not null,author varchar(255) not null,image_url varchar(255) not null);
 show tables;
 describe goodread;

#Test database in console
